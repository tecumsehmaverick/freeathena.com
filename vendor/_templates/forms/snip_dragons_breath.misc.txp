<!-- Hide duplicate links in tags list generated from tru_tags, smd_query -->
<script>
var seen = {};
$('.tags a').each(function() {
    var txt = $(this).text();
    if (seen[txt])
        $(this).remove();
    else
        seen[txt] = true;
});
</script>


<!-- get a tag icon in that tag list -->
